run             5000000
unfix           1

reset_timestep  0
fix             1 all nvt temp 300 300 100
dump            2 all custom 100000 uam-8b120c-nvt.lammpstrj id mol type x y z
dump_modify     2 sort id
run             500000
write_data      uam-8b120c-equi.lammps
unfix           1
